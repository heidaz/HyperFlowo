{
  "name": "solana-marketplace-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server.js",
  "bin": {
    "solana-marketplace-app": "server.js"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node server.js",
    "start:prod": "npm run build && node server.js",
    "update-deps": "npm install --package-lock-only && npm install",
    "railway": "npm install && npm run build && node server.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.19.4",
    "@metaplex-foundation/mpl-token-metadata": "^3.1.0",
    "@metaplex-foundation/umi": "^0.8.9",
    "@metaplex-foundation/umi-bundle-defaults": "^0.8.9",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.34",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.19",
    "@solana/web3.js": "^1.78.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "globals": "^13.24.0",
    "typescript": "^5.0.2",
    "vite-plugin-node-polyfills": "^0.16.0"
  },
  "engines": {
    "node": ">=18.0.0 <22.0.0",
    "npm": ">=9.0.0"
  }
}
