{
  "name": "solana-marketplace-app",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "server.js",
  "bin": {
    "solana-marketplace-app": "server.js"
  },
  "scripts": {
    "dev": "vite",
